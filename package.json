{
  "name": "music-feed",
  "version": "0.0.0",
  "description": "Simple music feed aggregator",
  "main": "index.js",
  "dependencies": {
    "autoprefixer": "^6.5.0",
    "bcrypt": "^0.8.7",
    "co-bcrypt": "^1.0.0",
    "co-request": "^0.2.1",
    "data.maybe": "^1.2.2",
    "elm": "^0.18.0",
    "jsonwebtoken": "^7.1.9",
    "knex": "^0.8.6",
    "koa": "^0.21.0",
    "koa-bodyparser": "^2.0.1",
    "koa-compress": "^1.0.9",
    "koa-jwt": "^1.2.0",
    "koa-mount": "^1.3.0",
    "koa-router": "^5.1.2",
    "koa-static": "^1.4.9",
    "koa-views": "^3.1.0",
    "lodash": "^3.10.1",
    "node-uuid": "^1.4.7",
    "pg": "^4.4.1",
    "postcss": "^5.2.4",
    "postcss-cli": "^2.6.0",
    "ramda": "^0.23.0",
    "superagent": "^1.3.0",
    "superagent-bluebird-promise": "^2.0.2",
    "uglify-js": "^2.7.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "intelli-espower-loader": "^1.0.1",
    "mocha": "^3.1.2",
    "power-assert": "^1.4.1",
    "supertest": "^2.0.0",
    "supertest-as-promised": "^4.0.0"
  },
  "scripts": {
    "test": "npm run test:client && npm run test:server",
    "test:client": "elm test elm-client/tests/Main.elm",
    "test:server": "NODE_ENV=test mocha --harmony server/tests/end-to-end/",
    "start": "node --harmony server.js",
    "setup": "knex migrate:latest",
    "postinstall": "npm run build:elm && npm run build:uglifyjs && npm run build:postcss",
    "build:elm": "cd elm-client && elm package install -y && npm run build:elm:radio && npm run build:elm:feed",
    "build:elm:radio": "cd elm-client && elm make src/Radio/Main.elm --output ../public/radio.js",
    "build:elm:feed": "cd elm-client && elm make src/Feed/Main.elm --output ../public/feed.js",
    "build:uglifyjs": "uglifyjs public/radio.js --compress --mangle --output public/radio.js",
    "build:postcss": "postcss --use autoprefixer -o public/style.css public/style.css"
  },
  "author": "kimlai",
  "license": "ISC"
}
